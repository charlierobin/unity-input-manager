	#tag Method, Flags = &h0
		Shared Function findFor(f as FolderItem) As WindowMain
		  var foundWindow as WindowMain = nil
		  
		  for each w as DesktopWindow in App.Windows()
		    
		    if w isA WindowMain then
		      
		      var mainWindow as WindowMain = WindowMain( w )
		      
		      if mainWindow.matches( f ) then
		        
		        foundWindow = mainWindow
		        
		        exit
		        
		      end if
		      
		    end if
		    
		  next
		  
		  return foundWindow
		  
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Function matches(f as FolderItem) As Boolean
		  if self.f is nil then return false
		  
		  return self.f.NativePath = f.NativePath
		  
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Shared Sub newDefault()
		  var asset as InputManagerAsset = new InputManagerAsset()
		  
		  WindowMain.newFor( asset, nil )
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Shared Sub newFor(asset as InputManagerAsset, f as FolderItem)
		  var w as WindowMain = new WindowMain( asset, f )
		  
		  #pragma unused w
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub save(f as FolderItem)
		  var asset as InputManagerAsset = InputManagerAsset.fromWindowMain( self.Axes.getAll(), self.CheckBoxUsePhysicalKeys.Value )
		  
		  var data as String = asset.toString()
		  
		  Files.safeSave( data, f, Files.CheckIfAlreadyExists.Yes, Files.LeaveTempFileFlag.No )
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub selectionChanged()
		  // Part of the Interface1 interface.
		  
		  if self.Axes.SelectedRowCount = 0 then
		    
		    self.ButtonDelete.Enabled = false
		    
		    self.ButtonDuplicate.Enabled = false
		    
		  else
		    
		    self.ButtonDelete.Enabled = true
		    
		    self.ButtonDuplicate.Enabled = true
		    
		  end if
		  
		  if self.Axes.SelectedRowCount = 1 then
		    
		    self.ButtonCopyForUnity.Enabled = true
		    
		  else
		    
		    self.ButtonCopyForUnity.Enabled = false
		    
		  end if
		  
		End Sub
	#tag EndMethod


