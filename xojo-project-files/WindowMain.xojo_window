	#tag MenuHandler
		Function Close() As Boolean Handles Close.Action
		  self.Close()
		  
		  return true
		  
		End Function
	#tag EndMenuHandler

	#tag MenuHandler
		Function Save() As Boolean Handles Save.Action
		  try
		    
		    if self.f is nil then
		      
		      var f as FolderItem = FolderItem.ShowSaveFileDialog( "", "InputManager.asset" )
		      
		      if not ( f is nil ) then
		        
		        self.f = f
		        
		        self.Title = if ( self.f is nil, "(Unsaved file)", self.f.NativePath )
		        
		        App.archiveExisting( self.f )
		        
		        self.save( self.f )
		        
		        self.Changed = false
		        
		      end if
		      
		    else
		      
		      App.archiveExisting( self.f )
		      
		      self.save( self.f )
		      
		      self.Changed = false
		      
		    end if
		    
		  catch e as RuntimeException
		    
		    MessageBox( e.Message )
		    
		  end try
		  
		  return true
		  
		  
		End Function
	#tag EndMenuHandler

	#tag MenuHandler
		Function SaveAs() As Boolean Handles SaveAs.Action
		  var f as FolderItem = FolderItem.ShowSaveFileDialog( "", "InputManager.asset" )
		  
		  if not ( f is nil ) then
		    
		    try
		      
		      self.f = f
		      
		      App.archiveExisting( self.f )
		      
		      self.save( self.f )
		      
		      self.Changed = false
		      
		    catch e as RuntimeException
		      
		      MessageBox( e.Message )
		      
		    end try
		    
		  end if
		  
		  return true
		  
		End Function
	#tag EndMenuHandler


	#tag Method, Flags = &h0
		Sub Constructor(asset as InputManagerAsset, f as FolderItem)
		  self.opening = true
		  
		  super.Constructor()
		  
		  self.f = f
		  
		  self.Title = if ( self.f is nil, "(Unsaved file)", self.f.NativePath )
		  
		  self.Axes.populate( asset.entries )
		  
		  self.CheckBoxUsePhysicalKeys.Value = asset.m_UsePhysicalKeys
		  
		  self.opening = false
		  
		  
		End Sub
	#tag EndMethod

