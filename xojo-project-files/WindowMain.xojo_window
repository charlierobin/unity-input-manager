#tag WindowCode
	#tag Event
		Function CancelClosing(appQuitting As Boolean) As Boolean
		  #pragma unused appQuitting
		  
		  if not self.Changed then return false
		  
		  // remember: returning false says everything is OK, returning true cancels
		  
		  var userResponse as Dialogs.UserResponse = Dialogs.UserResponse.Cancel
		  
		  if self.f is nil then
		    
		    userResponse = Dialogs.askUserAboutSaving( self )
		    
		    if userResponse = Dialogs.UserResponse.Save then
		      
		      self.f = FolderItem.ShowSaveFileDialog( "", "InputManager.asset" )
		      
		      if self.f is nil then userResponse = Dialogs.UserResponse.Cancel
		      
		    end if
		    
		  else
		    
		    userResponse = Dialogs.askUserAboutSaving( self )
		    
		  end if
		  
		  if userResponse = Dialogs.UserResponse.Cancel then return true
		  
		  if userResponse = Dialogs.UserResponse.Save then
		    
		    try
		      
		      App.archiveExisting( self.f )
		      
		      self.save( self.f )
		      
		      return false
		      
		    catch e as RuntimeException
		      
		      MessageBox( e.Message )
		      
		      return true
		      
		    end try
		    
		  elseif userResponse = Dialogs.UserResponse.DontSave then
		    
		    return false
		    
		  end if
		  
		  // should not get to this point ever, but just in case, return true by default
		  
		  raise new RuntimeException( "You really should never see this!" )
		  
		  return true
		  
		End Function
	#tag EndEvent

	#tag Event
		Sub MenuBarSelected()
		  EditCut.Enabled = self.Axes.SelectedRowCount > 0
		  
		  EditCopy.Enabled = self.Axes.SelectedRowCount > 0
		  
		  EditPaste.Enabled = self.Axes.SelectedRowCount = 0
		  
		  EditClear.Enabled = self.Axes.SelectedRowCount > 0
		  
		  
		End Sub
	#tag EndEvent

	#tag Event
