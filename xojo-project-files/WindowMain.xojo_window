#tag Events Axes
	#tag Event
		Sub DoublePressed()
		  if self.Axes.SelectedRowCount = 1 then
		    
		    var y as Integer = self.Axes.Top + ( self.Axes.RowHeight * self.Axes.SelectedRowIndex ) - ( self.Axes.RowHeight * self.Axes.ScrollPosition )
		    
		    var data as AxisSetting = self.Axes.RowTagAt( self.Axes.SelectedRowIndex )
		    
		    var w as WindowEdit = new WindowEdit( data, self )
		    
		    w.Top = Self.Top + y
		    
		    w.Left = self.Left + self.Axes.Left - 20
		    
		  end if
		  
		End Sub
	#tag EndEvent
