#tag WindowCode
	#tag MenuHandler
		Function Close() As Boolean Handles Close.Action
		  self.Close()
		  
		  return true
		  
		End Function
	#tag EndMenuHandler


	#tag Method, Flags = &h0
		Shared Sub open()
		  var aboutWindow as WindowAbout = nil
		  
		  for each w as DesktopWindow in App.Windows()
		    
		    if w isA WindowAbout then
		      
		      aboutWindow = WindowAbout( w )
		      
		      exit
		      
		    end if
		    
		  next
		  
		  if aboutWindow is nil then
		    
		    aboutWindow = new WindowAbout()
		    
		  else
		    
		    aboutWindow.Show()
		    
		  end if
		  
		End Sub
	#tag EndMethod


#tag EndWindowCode

